{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "QuantResearchAgent": "Debug"
    },
    "LogToFile": true,
    "LogFilePath": "./logs/agent.log"
  },
  "OpenAI": {
    "ApiKey": "YOUR_OPEN_AI_API_KEY",
    "ModelId": "gpt-4o-mini"
  },
  "YouTube": {
    "ApiKey": "YOUR_YOUTUBE_API_KEY"
  },
  "GoogleSearch": {
    "ApiKey": "YOUR_GOOGLE_SEARCH_API_KEY",
    "SearchEngineId": "YOUR_SEARCH_ENGINE_ID"
  },
  "Binance": {
    "ApiKey": "YOUR_BINANCE_API_KEY",
    "SecretKey": "YOUR_BINANCE_SECRET_KEY",
    "TestMode": true
  },
  "Alpaca": {
    "ApiKey": "YOUR_ALPACA_API_KEY",
    "SecretKey": "YOUR_ALPACA_SECRET_KEY",
    "IsPaperTrading": true,
    "BaseUrl": "https://paper-api.alpaca.markets"
  },
  "Agent": {
    "MaxConcurrentJobs": 5,
    "DataRefreshIntervalMinutes": 15,
    "PodcastAnalysisIntervalHours": 6,
    "RiskManagement": {
      "MaxDrawdown": 0.15,
      "VolatilityTarget": 0.12,
      "MaxPositions": 10,
      "PositionSizePercent": 0.1
    }
  },
  "NewsApi": {
    "BaseUrl": "http://127.0.0.1:5001"
  }
}